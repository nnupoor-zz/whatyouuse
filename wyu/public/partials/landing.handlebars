    <div class='container'>
    {{#each category}}
      <div class="mdl-grid g-card">
       <div class="md-cell mdl-cell--4-col mdl-cell--8-col-tablet">
        <div class="mdl-card mdl-shadow--2dp demo-card-square card--size">
        <div class="mdl-card__title mdl-card--expand {{name}}">
          <h2 class="mdl-card__title-text"></h2>
        </div>
        <div class="mdl-card__actions mdl-card--border" data-attr-category="{{_id}}">
          <a class="mdl-button mdl-js-button  mdl-button--accent mdl-js-ripple-effect" href="#/product/{{name}}">
            {{description}}
        </a>
        </div>
        </div>
        </div>
      </div>
      {{/each}}
      
    </div>
    <footer class="mdl-mini-footer footer--style"> 
        <span></span>
        <ul>
         <button class="mdl-button mdl-js-button mdl-js-ripple-effect">About</button>
         <button class="mdl-button mdl-js-button mdl-js-ripple-effect">Privacy</button>
        </ul>
    </footer>
</div>

