
      <main class="mdl-layout__content mdl-color--grey-A400 ">
      {{#each data}}
        <div class="mdl-grid demo-content">
            <div class="mdl-card mdl-cell mdl-cell--12-col-desktop mdl-cell--6-col-tablet mdl-cell--4-col-phone mdl-shadow--2dp">
              <div class="mdl-card__title mdl-color--blue-700">
                <div class="mdl-card__title-text">{{name}}</div>
              </div>
              <div class="mdl-card__supporting-text">
              
                <div class="demo-grid-2 mdl-grid">
                  <div class="mdl-cell mdl-cell--12-col-desktop mdl-cell--8-col-tablet mdl-cell--4-col-phone">
                    <h4> Properties </h4>
                    <div class="mdl-card__supporting-text">
                      {{Properties}}
                    </div>
                  </div>
                  <div class="mdl-cell mdl-cell--12-col-desktop mdl-cell--8-col-tablet mdl-cell--4-col-phone">
                    <h4> Effects </h4>
                    <div class="mdl-card__supporting-text">
                      {{Effects}}
                    </div>
                  </div>
                  <div class="mdl-cell mdl-cell--12-col-desktop mdl-cell--8-col-tablet mdl-cell--4-col-phone">
                    <h4> Side Effects </h4>
                    <div class="mdl-card__supporting-text">
                      {{SideEffects}}
                    </div>
                  </div>
                </div>
              </div>
              
              <div class="mdl-card__menu">
                <button class="mdl-button mdl-button--icon mdl-js-button mdl-js-ripple-effect">
                  <i class="material-icons">close</i>
                </button>
              </div>
            </div>  
        </div> 
      {{/each}}
      </main>    
   